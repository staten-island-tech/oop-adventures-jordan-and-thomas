        for i in range(len(data)):
            if firstpokemon == (data[i]["Name"]):
                playerteam.append(firstpokemon)
                firstpokemonnumber = i
                pokemonfound1 = 1
                break
        if pokemonfound1 == 0:
            for i in range(findnewpokemon1):
                firstpokemon = input("Try again ")
